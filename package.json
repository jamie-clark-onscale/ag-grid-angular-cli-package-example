  "angular-cli": {},
  "scripts": {
    "start": "ng serve --port 8080",
    "package": "NODE_OPTIONS=--openssl-legacy-provider ng build --aot --configuration production && npm run hash",
    "hash": "sh ./hashDirectory.sh > .hash",
    "lint": "tslint \"src/**/*.ts\"",
    "test:e2e": "NODE_OPTIONS=--openssl-legacy-provider ng test --watch false --browsers ChromeHeadless",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor"
  },
  "private": true,
  "dependencies": {
